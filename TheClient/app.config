<?xml version="1.0" encoding="utf-8" ?>
<configuration>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.6.1" />
    </startup>
    <system.serviceModel>
        <behaviors>
            <endpointBehaviors>
                <behavior name="">
                    <endpointErrorHandler />
                </behavior>
            </endpointBehaviors>
        </behaviors>
        <extensions>
            <behaviorExtensions>
                <add name="endpointErrorHandler" type="TheExtension.Endpoint.UnhandledErrorBehaviorExtension, TheExtension, Version=1.0.0.0, Culture=neutral, PublicKeyToken=null" />
            </behaviorExtensions>
        </extensions>
        <bindings>
            <basicHttpBinding>
                <binding name="httpBinding" />
            </basicHttpBinding>
            <netTcpBinding>
                <binding name="netTcpBinding" />
            </netTcpBinding>
        </bindings>
        <client>
            <endpoint address="http://localhost:42001/TheService/FaultyService/"
                binding="basicHttpBinding" bindingConfiguration="httpBinding"
                contract="FaultyService.IFaultyService" name="httpBinding" />
            <endpoint address="net.tcp://localhost:42002/TheService/FaultyService/"
                binding="netTcpBinding" bindingConfiguration="netTcpBinding"
                contract="FaultyService.IFaultyService" name="netTcpBinding">
                <identity>
                    <userPrincipalName value="okosmakov@universe.dart.spb" />
                </identity>
            </endpoint>
        </client>
    </system.serviceModel>
</configuration>